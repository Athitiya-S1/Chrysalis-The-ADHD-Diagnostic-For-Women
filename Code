<script>
    // This will load the interpretation when the user navigates to the interpretation page
    window.onload = function() {
    // Load results immediately on the results page
    if (window.location.hash === "#twenty-six") {
        getResults();
    }

    // Load interpretation immediately on the interpretation page
    if (window.location.hash === "#twenty-seven") {
        var interpretation = localStorage.getItem("interpretationResult");
        if (interpretation) {
            document.getElementById("interpretationText").innerHTML = interpretation;
        }
    }
};

// Detect when the hash changes (i.e., navigation between pages) to dynamically load content
window.onhashchange = function() {
    if (window.location.hash === "#twenty-seven") {
        var interpretation = localStorage.getItem("interpretationResult");
        if (interpretation) {
            document.getElementById("interpretationText").innerHTML = interpretation; // Display interpretation
        }
    }
};
</script>

<!-- RESOURCES PAGE -->
<div class="page" id="twenty-eight"> <!-- contains page content -->
	<div id="header" class="w3-container w3-flat-newheader"> <!-- contains header content -->
        <h1><strong>Resources</strong></h1> <!-- header title -->
    </div>
    <div id="content" class="w3-container">
        <p id=resources-blurb>Online autism tests can help with self-discovery and may guide you toward seeking a formal diagnosis. Although not definitive, these tests provide useful insights.
